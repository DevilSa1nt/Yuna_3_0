<Window x:Class="Yuna_3dmodel_Core_WPF.YunaOverlayWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:hx="clr-namespace:HelixToolkit.Wpf.SharpDX;assembly=HelixToolkit.Wpf.SharpDX"
        Title="YunaOverlayWindow"
        Width="800" Height="600"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        Background="Black"
        ResizeMode="NoResize"
        AllowsTransparency="False"
        ShowInTaskbar="False"
        Topmost="True">
    <Grid>
        <hx:Viewport3DX x:Name="viewport"
                        Background="Black"
                        ShowCoordinateSystem="True"
                        EnableRenderOrder="True" />
    </Grid>
</Window>
